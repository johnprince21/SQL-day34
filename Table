-- Create the 'products' table

CREATE TABLE products 
(
    id INT AUTO_INCREMENT PRIMARY KEY,
    products_name VARCHAR(255) NOT NULL,
    price DECIMAL(10, 2) NOT NULL,
    description TEXT NOT NULL
);

